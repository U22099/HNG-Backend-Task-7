generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model Document {
  id            String   @id @default(uuid())
  originalName  String
  mimeType      String
  size          Int
  s3Key         String
  extractedText String?
  summary       String?
  docType       String?
  metadata      String?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
